 1488: function getBDType( char ) {
 1489:   return char.match( TYPESET.char.biaodian.open )
 1490      ? 'bd-open'
 1491:     : char.match( TYPESET.char.biaodian.close )
 1492      ? 'bd-close bd-end'
 1493:     : char.match( TYPESET.char.biaodian.end )
 1494        ? (
 1495:         /(?:\u3001|\u3002|\uff0c)/i.test( char )
 1496          ? 'bd-end bd-cop'
 1497          : 'bd-end'
 1498        )
 1499:     : char.match(new RegExp( UNICODE.biaodian.liga ))
 1500:     ? 'bd-liga'
 1501:     : char.match(new RegExp( UNICODE.biaodian.middle ))
 1502      ? 'bd-middle'
 1503      : ''
