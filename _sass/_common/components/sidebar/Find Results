/Users/deep-core-01/Desktop/blog/wkddnjset.github.io/assets/css/main.scss:
    5  
    6  
    7: $scheme    : '{{ site.scheme | default: Muse }}';
    8: $variables : base $scheme custom;
    9: $mixins    : base $scheme custom;
   10  
   11: $jekyll-config-map : (
   12:   {% assign exclude = 'source, time, data, documents, github, related_posts, \
   13:     posts, pages, static_files, html_pages, html_files, collections, \
   14:     categories, tags, kramdown' | split: ', ' %}
   15:   {% assign names = '' | split: '' %}
   16:   {% assign values = '' | split: '' %}
   17:   {% assign levels = '' | split: '' %}
   18:   {% assign keys = '' | split: '' %}
   19  
   20:   {% for name in site %}
   21:     {% if exclude contains name %}
   22        {% continue %}
   23      {% endif %}
   24:     {% assign names = names | push: name %}
   25:     {% assign values = values | push: site[name] %}
   26:     {% assign levels = levels | push: 0 %}
   27    {% endfor %}
   28  
   29    {% for i in (1..1000) %}
   30:     {% if names.size == 0 %}
   31:       {% break %}
   32      {% endif %}
   33  
   34:     {% assign name = names | last %}
   35:     {% assign value = values | last %}
   36:     {% assign level = levels | last %}
   37  
   38:     {% assign names = names | pop %}
   39:     {% assign values = values | pop %}
   40:     {% assign levels = levels | pop %}
   41  
   42:     {% if name == '' or name == nil %}
   43        {% continue %}
   44      {% endif %}
   45  
   46:     {% assign keys = keys | slice: 0, level %}
   47:     {% assign keys = keys | push: name %}
   48  
   49:     {% assign value_type = value | jsonify | slice: 0 %}
   50:     {% if value_type == '{' %}
   51:       {% assign level = level | plus: 1 %}
   52        {% if level < 3 %}
   53:         {% for name_value in value %}
   54:           {% assign names = names | push: name_value[0] %}
   55:           {% assign values = values | push: name_value[1] %}
   56:           {% assign levels = levels | push: level %}
   57          {% endfor %}
   58        {% endif %}
   59:     {% elsif value_type != '[' %}
   60:       {% assign value = value | jsonify %}
   61:       {% if value != '{}' and value != '[]' %}
   62:         '{{ keys | join: "." }}': {{ value }},
   63        {% endif %}
   64      {% endif %}
   ..
   68  
   69  @function jekyll-config($key) {
   70:   @return map-get($jekyll-config-map, $key);
   71  }
