 1168  		_setDimension: function () {
 1169: 			var viewport   = F.getViewport(),
 1170  				steps      = 0,
 1171: 				canShrink  = false,
 1172: 				canExpand  = false,
 1173: 				wrap       = F.wrap,
 1174  				skin       = F.skin,
