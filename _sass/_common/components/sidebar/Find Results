 1297: 					height = getScalar(width / ratio);
 1298  				}
 1299  
 1300  				if (height < minHeight) {
 1301  					height = minHeight;
 1302: 					width  = getScalar(height * ratio);
 1303  				}
 1304  
 1305  			} else {
 1306: 				width = Math.max(minWidth, Math.min(width, maxWidth));
