  608:     if ( typeof attr !== 'object' )  return
  609:     var len = attr.length
  610  
  611:     // Native `NamedNodeMap``:
  612      if (
  613:       typeof attr[0] === 'object' &&
  614:       'name' in attr[0]
  615      ) {
  616:       for ( var i = 0; i < len; i++ ) {
  617:         if ( attr[ i ].value !== undefined ) {
  618:           target.setAttribute( attr[ i ].name, attr[ i ].value )
  619          }
  620        }
  621  
  622:     // Plain object:
  623      } else {
  624:       for ( var name in attr ) {
  625          if (
  626:           attr.hasOwnProperty( name ) &&
  627:           attr[ name ] !== undefined
  628          ) {
  629:           target.setAttribute( name, attr[ name ] )
  630          }
  631        }
  632      }
