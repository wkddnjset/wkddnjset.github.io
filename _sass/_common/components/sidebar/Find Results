  125:         wrap.className = 'fluid-vids';
  126:         wrap.style.position = 'relative';
  127:         wrap.style.marginBottom = '20px';
  128:         wrap.style.width = '100%';
  129:         wrap.style.paddingTop = videoRatio + '%';
  130  
  131:         // Add the iframe inside our newly created <div>
  132:         var iframeParent = iframe.parentNode;
  133:         iframeParent.insertBefore(wrap, iframe);
  134:         wrap.appendChild(iframe);
