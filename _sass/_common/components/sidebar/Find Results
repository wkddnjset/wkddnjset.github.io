/Users/deep-core-01/Desktop/blog/wkddnjset.github.io/search.xml:
    1  ---
    2: layout: null
    3: sitemap: false
    4  ---
    5  <?xml version="1.0" encoding="utf-8"?>
    6: <search>
    7    {% for post in site.posts %}
    8      <entry>
    9:       <title>{{ post.title | strip_html | xml_escape }}</title>
   10:       <url>{{ post.url | relative_url }}</url>
   11:       <content type="text">{{ post.content | strip_html | xml_escape }}</content>
   12:       <categories>
   13:         {% for category in post.categories %}
   14:           <category> {{ category | strip_html | xml_escape }} </category>
   15          {% endfor %}
   16:       </categories>
   17:       <tags>
   18:         {% for tag in post.tags %}
   19:           <tag> {{ tag | strip_html | xml_escape }} </tag>
   20          {% endfor %}
   21:       </tags>
   22:       <tags></tags>
   23      </entry>
   24    {% endfor %}
   25: </search>
