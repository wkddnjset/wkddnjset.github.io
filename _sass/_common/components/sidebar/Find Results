 1098:       var elementFilter = this.options.filterElements
 1099:       var forceContext = this.options.forceContext
 1100        return getText(this.node)
 1101        /**
 1102:        * Gets aggregate text of a node without resorting
 1103         * to broken innerText/textContent
 1104         */
 ....
 1106  
 1107          if (node.nodeType === 3) {
 1108:           return [node.data]
 1109          }
 1110  
 ....
 1113          }
 1114  
 1115:         var txt = ['']
 1116:         var i = 0
