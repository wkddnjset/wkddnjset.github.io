/Users/deep-core-01/Desktop/blog/wkddnjset.github.io/_includes/archive.html:
    1: <section id="posts" class="posts-collapse" hidden>
    2:   <span class="archive-move-on"></span>
    3  
    4:   <span class="archive-page-counter">
    5:     {% assign posts_length = site.posts.size %}
    6:     {% if posts_length > 210 %} {% assign cheers = 'excellent' %}
    7:       {% elsif posts_length > 130 %} {% assign cheers = 'great' %}
    8:       {% elsif posts_length > 80 %} {% assign cheers = 'good' %}
    9:       {% elsif posts_length > 50 %} {% assign cheers = 'nice' %}
   10:       {% elsif posts_length > 30 %} {% assign cheers = 'ok' %}
   11      {% else %}
   12:       {% assign cheers = 'um' %}
   13      {% endif %}
   14      {{ __.cheers[cheers] }}!
   15      {% if site.posts.size == 0 %}
   16:       {{ __.counter.archive_posts.zero }}
   17      {% elsif site.posts.size == 1 %}
   18:       {{ __.counter.archive_posts.one }}
   19      {% else %}
   20:       {{ __.counter.archive_posts.other | replace: '%d', site.posts.size }}
   21      {% endif %}
   22      {{ __.keep_on }}
   23:   </span>
   24  
   25:   {% assign paginate = site.archive.paginate | default: site.paginate  %}
   26:   {% assign paginate_path = site.archive.paginate_path | default: site.paginate_path  %}
   27  
   28    {% for post in site.posts %}
   29:     {% if paginate > 0 %}
   30:       {% assign page_num = forloop.index0 | divided_by: paginate | plus: 1 %}
   31:       {% if page_num == 1 %}
   32:         {% assign route = '/' %}
   33        {% else %}
   34:         {% assign route = paginate_path | replace: ':num', page_num %}
   35        {% endif %}
   36:       {% assign index0_modulo_paginate = forloop.index0 | modulo: paginate %}
   37      {% endif %}
   38  
   39:     {% comment %} Show year {% endcomment %}
   40:     {% assign post_year = post.date | date: '%Y' %}
   41:     {% if post_year != year or index0_modulo_paginate == 0 and index0_modulo_paginate %}
   42:       {% assign year = post_year %}
   43:       <div class="collection-title" {% if route %}route="{{ route }}"{% endif %}>
   44:         <h2 class="archive-year motion-element" id="archive-year-{{ year }}">{{ year }}</h2>
   45        </div>
   46      {% endif %}
   47      {% comment %} endshow {% endcomment %}
   48  
   49:     {% include _macro/post-collapse.html %}
   50  
   51    {% endfor %}
   ..
   53  </section>
   54  
   55: <nav class="pagination"></nav>
   56  

/Users/deep-core-01/Desktop/blog/wkddnjset.github.io/_includes/category.html:
    1: <section id="posts" class="posts-collapse" hidden>
    2:   <div class="collection-title">
    3      <{% if site.seo %}h2{% else %}h1{% endif %}>
    4:     {{ page.title }}
    5:     <small>{{  __.title.category  }}</small>
    6      </{% if site.seo %}h2{% else %}h1{% endif %}>
    7    </div>
    8  
    9:   {% assign paginate = site.category.paginate | default: site.paginate  %}
   10:   {% assign paginate_path = site.category.paginate_path | default: site.paginate_path  %}
   11  
   12:   {% for cat_posts in site.categories %}
   13:     {% assign cat = cat_posts[0] | url_encode | replace: '+', '%20' %}
   14:     {% assign posts = cat_posts[1] %}
   15  
   16      {% for post in posts %}
   17:       {% if paginate > 0 %}
   18:         {% assign page_num = forloop.index0 | divided_by: paginate | plus: 1 %}
   19:         {% assign route = '/' | append: cat %}
   20:         {% if page_num != 1 %}
   21:           {% assign route = route | append: paginate_path | replace: ':num', page_num %}
   22          {% endif %}
   23        {% endif %}
   24  
   25:       {% include _macro/post-collapse.html %}
   26      {% endfor %}
   27    {% endfor %}
   28  </section>
   29  
   30: <nav class="pagination"></nav>
