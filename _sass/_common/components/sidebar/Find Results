 1656        )
 1657      }
 1658:     if ( option.ellinika || option.greek ) {
 1659:       this.wrap(
 1660:         TYPESET.char.ellinika,
 1661:         getFuncOrElmt( option.ellinika || option.greek )
 1662            ||
 1663:         $.clone($.create( 'h-char', 'alphabet ellinika greek' ))
 1664        )
 1665      }
 1666:     if ( option.kirillica || option.cyrillic ) {
 1667:       this.wrap(
 1668:         TYPESET.char.kirillica,
 1669:         getFuncOrElmt( option.kirillica || option.cyrillic )
 1670            ||
 1671:         $.clone($.create( 'h-char', 'alphabet kirillica cyrillic' ))
 1672        )
 1673      }
 1674:     if ( option.kana ) {
 1675:       this.wrap(
 1676:         TYPESET.char.kana,
 1677:         getFuncOrElmt( option.kana )
 1678            ||
 1679:         $.clone($.create( 'h-char', 'kana' ))
 1680        )
 1681      }
 1682:     if ( option.eonmun || option.hangul ) {
 1683:       this.wrap(
 1684:         TYPESET.char.eonmun,
 1685:         getFuncOrElmt( option.eonmun || option.hangul )
 1686            ||
 1687:         $.clone($.create( 'h-char', 'eonmun hangul' ))
 1688        )
 1689      }
 1690  
 1691:     this.endAvoid()
 1692      return this
 1693    }
 1694  })
