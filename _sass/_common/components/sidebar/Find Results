/Users/deep-core-01/Desktop/blog/wkddnjset.github.io/_includes/_helper/open_graph.html:
    1: {% comment %} open_graph(twitter_id, google_plus, fb_admins, fb_app_id) {% endcomment %}
    2: {% capture open_graph %}
    3:   {% assign default_images = '' | split: '' %}
    4:   {% assign images = page.photos | default: default_images %}
    5:   {% assign page_content = page.content %}
    6:   {% assign description = page.description | default: page.excerpt | default: page_content | default: site.description %}
    7  
    8:   {% if page.tags and page.tags.size != 0 %}{% assign page_tags = page.tags | join: ', ' %}{% endif %}
    9:   {% assign keywords = page.keywords | default: page_tags | default: site.keywords %}
   10  
   11:   {% if page.layout == 'post' %}
   12:     {% assign type = 'article' %}
   13    {% else %}
   14:     {% assign type = 'website' %}
   15    {% endif %}
   16  
   17:   {% assign url = page.url | absolute_url %}
   18:   {% assign og_title = page.title | default: site.title %}
   19:   {% assign site_name = site.title %}
   20:   {% assign language = page.language | default: site.language %}
   21:   {% assign updated = page.updated %}
   22:   {% assign twitter_card = 'summary' %}
   23  
   24    {% if description %}
   25:     {% assign description = description | strip_html | strip |
   26:                             replace: '<', '&lt;' |
   27:                             replace: '>', '&gt;' |
   28:                             replace: '&', '&amp;' |
   29:                             replace: '"', '&quot;' |
   30:                             replace: "'", '&apos;' |
   31:                             replace: '\n', ' ' %}
   32    {% endif %}
   33  
   34:   {% if images.size == 0 and page_content %}
   35:     {% assign imgs = page_content | strip | split: '<img' | shift %}
   36      {% for img in imgs %}
   37:       {% assign tmp = img | strip | split: 'src=' | shfit %}
   38:       {% assign tmp = tmp | strip | split: '\"' | shift %}
   39:       {% assign image = tmp[0] | strip %}
   40:       {% if image %}
   41:         {% assign images = images | push: image %}
   42        {% endif %}
   43      {% endfor %}
   ..
   45  
   46    {% if description %}
   47:     <meta name="description" content="{{ description }}">
   48      <!--newline-->
   49    {% endif %}
   50  
   51    {% if keywords %}
   52:     <meta name="keywords" content="{{ keywords | xml_escape }}">
   53      <!--newline-->
   54    {% endif %}
   55  
   56:   <meta property="og:type" content="{{ type | xml_escape }}"><!--newline-->
   57:   <meta property="og:title" content="{{ og_title | xml_escape }}"><!--newline-->
   58:   <meta property="og:url" content="{{ url }}"><!--newline-->
   59:   <meta property="og:site_name" content="{{ site_name | xml_escape }}"><!--newline-->
   60  
   61    {% if description %}
   62:     <meta property="og:description" content="{{ description }}">
   63      <!--newline-->
   64    {% endif %}
   65  
   66:   {% if language %}
   67:     <meta property="og:locale" content="{{ language }}">
   68      <!--newline-->
   69    {% endif %}
   70  
   71:   {% for image in images %}
   72:     <meta property="og:image" content="{{ image }}">
   73      <!--newline-->
   74    {% endfor %}
   75  
   76:   {% if updated %}
   77:     <meta property="og:updated_time" content="{{ updated | date_to_xmlschema }}">
   78      <!--newline-->
   79    {% endif %}
   80  
   81:   <meta name="twitter:card" content="{{ twitter_card | xml_escape }}"><!--newline-->
   82:   <meta name="twitter:title" content="{{ og_title | xml_escape }}"><!--newline-->
   83  
   84    {% if description %}
   85:     <meta name="twitter:description" content="{{ description }}">
   86      <!--newline-->
   87    {% endif %}
   88  
   89:   {% if images.size != 0 %}
   90:     <meta name="twitter:image" content="{{ images[0] }}">
   91      <!--newline-->
   92    {% endif %}
   ..
   94    {% if twitter_id %}
   95      {% if twitter_id[0] != '@' %}
   96:       {% assign twitter_id = '@' | append: twitter_id %}
   97      {% endif %}
   98:     <meta name="twitter:creator" content="{{ twitter_id | xml_escape }}">
   99      <!--newline-->
  100    {% endif %}
  101  
  102    {% if twitter_site %}
  103:     <meta name="twitter:site" content="{{ twitter_site | xml_escape }}">
  104      <!--newline-->
  105    {% endif %}
