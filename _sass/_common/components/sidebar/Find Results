 2271  /**
 2272:  * Normalisation rendering mechanism
 2273   */
 2274: $.extend( Locale, {
 2275  
 2276:   // Render and normalise the given context by routine:
 2277    //
 2278    // ruby -> u, ins -> s, del -> em
 ....
 2285    },
 2286  
 2287:    // Traverse all target elements and address
 2288:    // presentational corrections if any two of
 2289:    // them are adjacent to each other.
 2290:   renderDecoLine: function( context, target ) {
 2291:     var $$target = $.qsa( target || 'u, ins', context )
 2292:     var i = $$target.length
 2293  
 2294:     traverse: while ( i-- ) {
 2295:       var $this = $$target[ i ]
 2296:       var $prev = null
 2297  
 2298:       // Ignore all `<wbr>` and comments in between,
 2299:       // and add class `.adjacent` once two targets
 2300:       // are next to each other.
 2301        ignore: do {
 2302          $prev = ( $prev || $this ).previousSibling
 2303  
 2304          if ( !$prev ) {
 2305:           continue traverse
 2306:         } else if ( $$target[ i-1 ] === $prev ) {
 2307:           $this.classList.add( 'adjacent' )
 2308          }
 2309:       } while ( $.isIgnorable( $prev ))
 2310      }
 2311    },
 2312  
 2313:   // Traverse all target elements to render
 2314:   // emphasis marks.
 2315:   renderEm: function( context, target ) {
 2316:     var method = target ? 'qsa' : 'tag'
 2317:     var target = target || 'em'
 2318:     var $target = $[ method ]( target, context )
 2319  
 2320:     $target
 2321:     .forEach(function( elem ) {
 2322:       var $elem = Han( elem )
 2323  
 2324:       if ( Locale.support.textemphasis ) {
 2325          $elem
 2326:         .avoid( 'rt, h-char' )
 2327:         .charify({ biaodian: true, punct: true })
 2328        } else {
 2329          $elem
 2330:         .avoid( 'rt, h-char, h-char-group' )
 2331          .jinzify()
 2332          .groupify({ western: true })
 2333:         .charify({
 2334:           hanzi:     true,
 2335:           biaodian:  true,
 2336            punct:     true,
 2337:           latin:     true,
 2338:           ellinika:  true,
 2339:           kirillica: true
 2340          })
 2341        }
 ....
 2344  })
