   88:   embeddedVideoTransformer: function () {
   89:     var $iframes = $('iframe');
   90  
   91:     // Supported Players. Extend this if you need more players.
   92:     var SUPPORTED_PLAYERS = [
   93        'www.youtube.com',
   94:       'player.vimeo.com',
   95:       'player.youku.com',
   96        'music.163.com',
   97        'www.tudou.com'
