   10:       {{ __.title.categories | append: page_title_suffix }}
   11:     {% elsif page.type == "tags" and page.title == nil %}
   12:       {{ __.title.tags | append: page_title_suffix }}
   13:     {% elsif page.type == "about" and page.title == nil %}
   14:       {{ __.title.about | append: page_title_suffix }}
   15      {% else %}
   16:       {{ page.title | append: page_title_suffix }}
   17      {% endif %}
   18:   {% endcapture %}
   19: {% when 'archive' %}
   20:   {% capture title %}{{ __.title.archives }} | {{ site.title }}{% endcapture %}
   21: {% when 'category' %}
   22:   {% capture title %}{{ __.title.category }} | {{ site.title }}{% endcapture %}
   23: {% when 'tag' %}
   24:   {% capture title %}{{ __.title.tag }} | {{ site.title }}{% endcapture %}
   25  {% when 'schedule' %}
   26:   {% capture title %}{{ __.title.schedule }} | {{ site.title }}{% endcapture %}
   27: {% endcase %}
