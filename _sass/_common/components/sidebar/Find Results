   56: 					attributes.expires = expires;
   57  				}
   58  
   59: 				// We're using "expires" because "max-age" is not supported by IE
   60: 				attributes.expires = attributes.expires ? attributes.expires.toUTCString() : '';
   61  
   62  				try {
   63: 					result = JSON.stringify(value);
   64  					if (/^[\{\[]/.test(result)) {
   65: 						value = result;
   66  					}
