  995    exposed.NON_INLINE_PROSE = function(el) {
  996:     return hasOwn.call(exposed.NON_CONTIGUOUS_PROSE_ELEMENTS, el.nodeName.toLowerCase())
  997    }
  998:   // Presets accessed via `options.preset` when calling findAndReplaceDOMText():
  999    exposed.PRESETS = {
 1000      prose: {
 1001        forceContext: exposed.NON_INLINE_PROSE,
 1002        filterElements: function(el) {
 1003:         return !hasOwn.call(exposed.NON_PROSE_ELEMENTS, el.nodeName.toLowerCase())
 1004        }
