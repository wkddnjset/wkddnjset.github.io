 2989:   // Consider ‘a configured context’ the special
 2990:   // case of the default situation. Will have to
 2991:   // replace the `Han.init` with the instance as
 2992:   // well (for future usage).
 2993:   } else if ( initContext = document.querySelector( '.han-init-context' )) {
 2994:     Han.init = Han( initContext ).render()
 2995    }
 2996  })
 2997  
 2998: // Expose to global namespace
 2999: if ( typeof noGlobalNS === 'undefined' || noGlobalNS === false ) {
 3000:   window.Han = Han
 3001  }
 3002  
 3003: return Han
 3004  });
 3005  

