 1135                // not considered their own contexts:
 1136                // I.e. ['some', ['thing']] -> ['something']
 ....
 1149  
 1150      /** 
 1151:      * Steps through the target node, looking for matches, and
 1152:      * calling replaceFn when a match is found.
 1153       */
 1154:     processMatches: function() {
 1155  
 1156:       var matches = this.matches
 1157:       var node = this.node
 1158:       var elementFilter = this.options.filterElements
 1159:       var startPortion,
 1160          endPortion,
 1161          innerPortions = [],
 1162          curNode = node,
 1163:         match = matches.shift(),
 1164:         atIndex = 0, // i.e. nodeAtIndex
 1165:         matchIndex = 0,
 1166          portionIndex = 0,
 1167:         doAvoidNode,
 1168:         nodeStack = [node]
 1169        out: while (true) {
 1170  
 1171          if (curNode.nodeType === 3) {
 1172  
 1173:           if (!endPortion && curNode.length + atIndex >= match.endIndex) {
 1174  
 1175              // We've found the ending
 ....
 1177                node: curNode,
 1178                index: portionIndex++,
 1179:               text: curNode.data.substring(match.startIndex - atIndex, match.endIndex - atIndex),
 1180:               indexInMatch: atIndex - match.startIndex,
 1181:               indexInNode: match.startIndex - atIndex, // always zero for end-portions
 1182:               endIndexInNode: match.endIndex - atIndex,
 1183                isEnd: true
 1184              }
 1185:           } else if (startPortion) {
 1186              // Intersecting node
 1187              innerPortions.push({
 1188                node: curNode,
 1189                index: portionIndex++,
 1190:               text: curNode.data,
 1191:               indexInMatch: atIndex - match.startIndex,
 1192:               indexInNode: 0 // always zero for inner-portions
 1193              })
 1194            }
 1195  
 1196:           if (!startPortion && curNode.length + atIndex > match.startIndex) {
 1197:             // We've found the match start
 1198:             startPortion = {
 1199                node: curNode,
 1200                index: portionIndex++,
 1201:               indexInMatch: 0,
 1202:               indexInNode: match.startIndex - atIndex,
 1203:               endIndexInNode: match.endIndex - atIndex,
 1204:               text: curNode.data.substring(match.startIndex - atIndex, match.endIndex - atIndex)
 1205              }
 1206            }
 1207  
 1208:           atIndex += curNode.data.length
 1209          }
 1210  
 1211:         doAvoidNode = curNode.nodeType === 1 && elementFilter && !elementFilter(curNode)
 1212:         if (startPortion && endPortion) {
 1213  
 1214:           curNode = this.replaceMatch(match, startPortion, innerPortions, endPortion)
 1215:           // processMatches has to return the node that replaced the endNode
 1216:           // and then we step back so we can continue from the end of the 
 1217:           // match:
 1218  
 1219:           atIndex -= (endPortion.node.data.length - endPortion.endIndexInNode)
 1220:           startPortion = null
 1221            endPortion = null
 1222            innerPortions = []
 1223:           match = matches.shift()
 1224            portionIndex = 0
 1225:           matchIndex++
 1226:           if (!match) {
 1227:             break; // no more matches
 1228            }
 1229  
 1230          } else if (
 1231:           !doAvoidNode &&
 1232            (curNode.firstChild || curNode.nextSibling)
 1233          ) {
 1234:           // Move down or forward:
 1235            if (curNode.firstChild) {
 1236:             nodeStack.push(curNode)
 1237              curNode = curNode.firstChild
 1238            } else {
 ....
 1242          }
 1243  
 1244:         // Move forward or up:
 1245          while (true) {
 1246            if (curNode.nextSibling) {
 1247              curNode = curNode.nextSibling
 1248:             break
 1249            }
 1250:           curNode = nodeStack.pop()
 1251            if (curNode === node) {
 1252:             break out
 1253            }
 1254          }
 ....
 1262       */
 1263      revert: function() {
 1264:       // Reversion occurs backwards so as to avoid nodes subsequently
 1265:       // replaced during the matching phase (a forward process):
 1266:       for (var l = this.reverts.length; l--;) {
 1267          this.reverts[l]()
 1268        }
 ....
 1270      },
 1271  
 1272:     prepareReplacementString: function(string, portion, match, matchIndex) {
 1273:       var portionMode = this.options.portionMode
 1274        if (
 1275          portionMode === PORTION_MODE_FIRST &&
 1276:         portion.indexInMatch > 0
 1277        ) {
 1278          return ''
 1279        }
 1280:       string = string.replace(/\$(\d+|&|`|')/g, function($0, t) {
 1281:         var replacement
 1282          switch(t) {
 1283:           case '&':
 1284:             replacement = match[0]
 1285:             break
 1286:           case '`':
 1287:             replacement = match.input.substring(0, match.startIndex)
 1288:             break
 1289:           case '\'':
 1290:             replacement = match.input.substring(match.endIndex)
 1291:             break
 1292:           default:
 1293:             replacement = match[+t]
 1294          }
 1295:         return replacement
 1296        })
 1297        if (portionMode === PORTION_MODE_FIRST) {
 ....
 1300  
 1301        if (portion.isEnd) {
 1302:         return string.substring(portion.indexInMatch)
 1303        }
 1304  
 1305:       return string.substring(portion.indexInMatch, portion.indexInMatch + portion.text.length)
 1306      },
 1307  
 1308:     getPortionReplacementNode: function(portion, match, matchIndex) {
 1309  
 1310:       var replacement = this.options.replace || '$&'
 1311:       var wrapper = this.options.wrap
 1312:       if (wrapper && wrapper.nodeType) {
 1313:         // Wrapper has been provided as a stencil-node for us to clone:
 1314:         var clone = doc.createElement('div')
 1315:         clone.innerHTML = wrapper.outerHTML || new XMLSerializer().serializeToString(wrapper)
 1316:         wrapper = clone.firstChild
 1317        }
 1318  
 1319:       if (typeof replacement == 'function') {
 1320:         replacement = replacement(portion, match, matchIndex)
 1321:         if (replacement && replacement.nodeType) {
 1322:           return replacement
 1323          }
 1324:         return doc.createTextNode(String(replacement))
 1325        }
 1326  
 1327:       var el = typeof wrapper == 'string' ? doc.createElement(wrapper) : wrapper
 1328:       replacement = doc.createTextNode(
 1329:         this.prepareReplacementString(
 1330:           replacement, portion, match, matchIndex
 1331          )
 1332        )
 1333:       if (!replacement.data) {
 1334:         return replacement
 1335        }
 1336  
 1337        if (!el) {
 1338:         return replacement
 1339        }
 1340  
 1341:       el.appendChild(replacement)
 1342        return el
 1343      },
 1344  
 1345:     replaceMatch: function(match, startPortion, innerPortions, endPortion) {
 1346  
 1347:       var matchStartNode = startPortion.node
 1348:       var matchEndNode = endPortion.node
 1349:       var preceedingTextNode
 1350:       var followingTextNode
 1351:       if (matchStartNode === matchEndNode) {
 1352  
 1353:         var node = matchStartNode
 1354:         if (startPortion.indexInNode > 0) {
 1355:           // Add `before` text node (before the match)
 1356:           preceedingTextNode = doc.createTextNode(node.data.substring(0, startPortion.indexInNode))
 1357:           node.parentNode.insertBefore(preceedingTextNode, node)
 1358          }
 1359  
 1360:         // Create the replacement node:
 1361:         var newNode = this.getPortionReplacementNode(
 1362            endPortion,
 1363:           match
 1364          )
 1365:         node.parentNode.insertBefore(newNode, node)
 1366          if (endPortion.endIndexInNode < node.length) { // ?????
 1367:           // Add `after` text node (after the match)
 1368:           followingTextNode = doc.createTextNode(node.data.substring(endPortion.endIndexInNode))
 1369:           node.parentNode.insertBefore(followingTextNode, node)
 1370          }
 1371  
 1372:         node.parentNode.removeChild(node)
 1373          this.reverts.push(function() {
 1374            if (preceedingTextNode === newNode.previousSibling) {
 1375:             preceedingTextNode.parentNode.removeChild(preceedingTextNode)
 1376            }
 1377            if (followingTextNode === newNode.nextSibling) {
 1378:             followingTextNode.parentNode.removeChild(followingTextNode)
 1379            }
 1380:           newNode.parentNode.replaceChild(node, newNode)
 1381          })
 1382          return newNode
 1383        } else {
 1384:         // Replace matchStartNode -> [innerMatchNodes...] -> matchEndNode (in that order)
 1385  
 1386:         preceedingTextNode = doc.createTextNode(
 1387:           matchStartNode.data.substring(0, startPortion.indexInNode)
 1388          )
 1389:         followingTextNode = doc.createTextNode(
 1390:           matchEndNode.data.substring(endPortion.endIndexInNode)
 1391          )
 1392:         var firstNode = this.getPortionReplacementNode(
 1393:           startPortion,
 1394:           match
 1395          )
 1396:         var innerNodes = []
 1397:         for (var i = 0, l = innerPortions.length; i < l; ++i) {
 1398:           var portion = innerPortions[i]
 1399:           var innerNode = this.getPortionReplacementNode(
 1400              portion,
 1401:             match
 1402            )
 1403:           portion.node.parentNode.replaceChild(innerNode, portion.node)
 1404            this.reverts.push((function(portion, innerNode) {
 1405              return function() {
 1406:               innerNode.parentNode.replaceChild(portion.node, innerNode)
 1407              }
 1408            }(portion, innerNode)))
 ....
 1410          }
 1411  
 1412:         var lastNode = this.getPortionReplacementNode(
 1413            endPortion,
 1414:           match
 1415          )
 1416:         matchStartNode.parentNode.insertBefore(preceedingTextNode, matchStartNode)
 1417:         matchStartNode.parentNode.insertBefore(firstNode, matchStartNode)
 1418:         matchStartNode.parentNode.removeChild(matchStartNode)
 1419:         matchEndNode.parentNode.insertBefore(lastNode, matchEndNode)
 1420:         matchEndNode.parentNode.insertBefore(followingTextNode, matchEndNode)
 1421:         matchEndNode.parentNode.removeChild(matchEndNode)
 1422          this.reverts.push(function() {
 1423:           preceedingTextNode.parentNode.removeChild(preceedingTextNode)
 1424:           firstNode.parentNode.replaceChild(matchStartNode, firstNode)
 1425:           followingTextNode.parentNode.removeChild(followingTextNode)
 1426:           lastNode.parentNode.replaceChild(matchEndNode, lastNode)
 1427          })
 1428:         return lastNode
 1429        }
