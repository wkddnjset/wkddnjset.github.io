  669:       typeof target === 'object' ||
  670:       typeof target === 'function' ) &&
  671        typeof object === 'object'
  672      ) {
  673:       for ( var name in object ) {
  674:         if (object.hasOwnProperty( name )) {
  675:           target[ name ] = object[ name ]
  676          }
  677        }
