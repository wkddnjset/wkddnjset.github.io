 1977  // 1. Simple ruby polyfill;
 1978: // 2. Inter-character polyfill for Zhuyin
 1979  function renderSimpleRuby( $ruby ) {
 1980:   var frag = $.create( '!' )
 1981:   var clazz = $ruby.classList
 1982:   var $rb, $ru
 1983  
 1984:   frag.appendChild( $.clone( $ruby ))
 1985  
 1986    $
 1987:   .tag( 'rt', frag.firstChild )
 1988:   .forEach(function( $rt ) {
 1989:     var $rb = $.create( '!' )
 1990:     var airb = []
 1991:     var irb
 1992  
 1993      // Consider the previous nodes the implied
 1994:     // ruby base
 1995      do {
 1996        irb = ( irb || $rt ).previousSibling
 1997:       if ( !irb || irb.nodeName.match( /((?:h\-)?r[ubt])/i ))  break
 1998  
 1999        $rb.insertBefore( $.clone( irb ), $rb.firstChild )
 2000:       airb.push( irb )
 2001:     } while ( !irb.nodeName.match( /((?:h\-)?r[ubt])/i ))
 2002  
 2003:     // Create a real `<h-ru>` to append.
 2004:     $ru = clazz.contains( 'zhuyin' ) ? createZhuyinRu( $rb, $rt ) : createNormalRu( $rb, $rt )
 2005  
 2006:     // Replace the ruby text with the new `<h-ru>`,
 2007:     // and remove the original implied ruby base(s)
 2008      try {
 2009:       $rt.parentNode.replaceChild( $ru, $rt )
 2010:       airb.map( $.remove )
 2011:     } catch ( e ) {}
 2012    })
 2013:   return createCustomRuby( frag )
 2014  }
 2015  
 2016: function renderInterCharRuby( $ruby ) {
 2017:   var frag = $.create( '!' )
 2018:   frag.appendChild( $.clone( $ruby ))
 2019  
 2020    $
 2021:   .tag( 'rt', frag.firstChild )
 2022:   .forEach(function( $rt ) {
 2023:     var $rb = $.create( '!' )
 2024:     var airb = []
 2025:     var irb, $zhuyin
 2026  
 2027      // Consider the previous nodes the implied
 2028:     // ruby base
 2029      do {
 2030        irb = ( irb || $rt ).previousSibling
 2031:       if ( !irb || irb.nodeName.match( /((?:h\-)?r[ubt])/i ))  break
 2032  
 2033        $rb.insertBefore( $.clone( irb ), $rb.firstChild )
 2034:       airb.push( irb )
 2035:     } while ( !irb.nodeName.match( /((?:h\-)?r[ubt])/i ))
 2036  
 2037:     $zhuyin = $.create( 'rt' )
 2038      $zhuyin.innerHTML = getZhuyinHTML( $rt )
 2039:     $rt.parentNode.replaceChild( $zhuyin, $rt )
 2040    })
 2041:   return frag.firstChild
 2042  }
 2043  
 2044  // 3. Complex ruby polyfill
 2045: // - Double-lined annotation;
 2046: // - Right-angled annotation.
 2047  function renderComplexRuby( $ruby ) {
 2048:   var frag = $.create( '!' )
 2049:   var clazz = $ruby.classList
 2050:   var $cloned, $rb, $ru, maxspan
 2051  
 2052:   frag.appendChild( $.clone( $ruby ))
 2053:   $cloned = frag.firstChild
 2054  
 2055:   $rb = $ru = $.tag( 'rb', $cloned )
 2056:   maxspan = $rb.length
 2057  
 2058:   // First of all, deal with Zhuyin containers
 2059:   // individually
 2060    //
 2061:   // Note that we only support one single Zhuyin
 2062:   // container in each complex ruby
 2063    void function( $rtc ) {
 2064      if ( !$rtc )  return
 2065  
 2066      $ru = $
 2067:       .tag( 'rt', $rtc )
 2068:       .map(function( $rt, i ) {
 2069          if ( !$rb[ i ] )  return
 2070:         var ret = createZhuyinRu( $rb[ i ], $rt )
 2071  
 2072          try {
 2073:           $rb[ i ].parentNode.replaceChild( ret, $rb[ i ] )
 2074:         } catch ( e ) {}
 2075          return ret
 2076        })
 2077  
 2078:     // Remove the container once it's useless
 2079      $.remove( $rtc )
 2080:     $cloned.setAttribute( 'rightangle', 'true' )
 2081    }( $cloned.querySelector( 'rtc.zhuyin' ))
 2082  
 2083:   // Then, normal annotations other than Zhuyin
 2084    $
 2085:   .qsa( 'rtc:not(.zhuyin)', $cloned )
 2086:   .forEach(function( $rtc, order ) {
 2087:     var ret
 2088      ret = $
 2089:       .tag( 'rt', $rtc )
 2090:       .map(function( $rt, i ) {
 2091:         var rbspan = Number( $rt.getAttribute( 'rbspan' ) || 1 )
 2092:         var span = 0
 2093:         var aRb = []
 2094:         var $rb, ret
 2095  
 2096:         if ( rbspan > maxspan )  rbspan = maxspan
 2097  
 2098          do {
 2099            try {
 2100              $rb = $ru.shift()
 2101:             aRb.push( $rb )
 2102:           } catch (e) {}
 2103  
 2104:           if ( typeof $rb === 'undefined' )  break
 2105:           span += Number( $rb.getAttribute( 'span' ) || 1 )
 2106:         } while ( rbspan > span )
 2107  
 2108:         if ( rbspan < span ) {
 2109:           if ( aRb.length > 1 ) {
 2110:             console.error( 'An impossible `rbspan` value detected.', ruby )
 2111              return
 2112            }
 2113:           aRb  = $.tag( 'rb', aRb[0] )
 2114:           $ru  = aRb.slice( rbspan ).concat( $ru )
 2115:           aRb  = aRb.slice( 0, rbspan )
 2116:           span = rbspan
 2117          }
 2118  
 2119:         ret = createNormalRu( aRb, $rt, {
 2120:           'class': clazz,
 2121:           span: span,
 2122            order: order
 2123          })
 2124  
 2125          try {
 2126:           aRb[0].parentNode.replaceChild( ret, aRb.shift() )
 2127:           aRb.map( $.remove )
 2128:         } catch (e) {}
 2129          return ret
 2130        })
 2131      $ru = ret
 2132:     if ( order === 1 )  $cloned.setAttribute( 'doubleline', 'true' )
 2133  
 2134:     // Remove the container once it's useless
 2135      $.remove( $rtc )
 2136    })
 2137:   return createCustomRuby( frag )
 2138  }
