/Users/deep-core-01/Desktop/blog/wkddnjset.github.io/_includes/_scripts/pages/pagination.html:
    1: {% assign paginate_path = site[page.layout].paginate_path | default: site.paginate_path  %}
    2: {% assign paginate = site[page.layout].paginate | default: site.paginate  %}
    3  
    4: {% if paginate > 0 %}
    5:   <script type="text/javascript" src="{{ site.js | relative_url }}/src/paginator.js"></script>
    6    <script>
    7      (function() {
    8        // Url route
    9:       var hash = window.location.hash.substring(1);
   10:       $(window).on('hashchange', function() {
   11:         location.reload();
   12          $(this).scrollTop(0);
   13        });
   14  
   15:       if (!hash)
   16:         hash = "/";
   17:       $('[route][route!="' + hash + '"]').remove();
   18        $('#posts').show();
   19  
   20:       // Pagination
   21:       {% if page.layout == 'category' %}
   22:         {% assign collections = site.categories %}
   23:         {% assign redirect = '/categories/' | relative_url %}
   24        {% endif %}
   25  
   26:       {% if page.layout == 'tag' %}
   27:         {% assign collections = site.tags %}
   28:         {% assign redirect = '/tags/' | relative_url %}
   29        {% endif %}
   30  
   31        {% if collections %}
   32:         if (hash == "/")  {
   33:           window.location = "{{ redirect }}";
   34            return;
   35          }
   36  
   37:         {% capture maps %}
   38:           {% for name_posts in collections %}
   39:             '{{ name_posts[0] | url_encode | replace: '+', '%20' }}': {{ name_posts[1].size }},
   40            {% endfor %}
   41:         {% endcapture %}
   42:         {% assign maps = maps | normalize_whitespace %}
   43  
   44:         var maps = { {{ maps }} };
   45:         var dirs = hash.split("/");
   46:         var collection = decodeURIComponent(dirs[1]);
   47          dirs.splice(0, 2);
   48:         hash = "/" + dirs.join("/");
   49  
   50          if (!collection) {
   51:           window.location = "{{ redirect }}";
   52            return;
   53          }
   54  
   55:         $('.collection-title')[0].childNodes[1].childNodes[0].nodeValue = collection;
   56:         document.title = "{{ __.title[page.layout] }}: " + collection + " | {{ site.title }}";
   57  
   58:         var sizes = maps[collection];
   59:         var base = "#" + "/" + collection;
   60        {% else %}
   61:         var sizes = {{ site.posts.size }}
   62:         var base = "#";
   63        {% endif %}
   64  
   65:       var current = 0;
   66  
   67:       if (hash != '/') {
   68:         {% capture regex %}/(?:{{ paginate_path | replace: ':num', '(\d+)' | replace: '/', '\\/' }})/{% endcapture %}
   69  
   70:         var reg = {{ regex }};
   71:         var res = reg.exec(hash);
   72  
   73          if (res && res[1])
   74:           current = parseInt(res[1]);
   75        }
   76        else {
   ..
   78        }
   79  
   80:       var total = Math.floor(sizes / {{ paginate }});
   81:       if (sizes % {{ paginate }} > 0) {
   82:         total = total + 1;
   83        }
   84  
   85:       if (total <= 1) {
   86:         $('nav.pagination').remove();
   87        }
   88        else {
   89:         var content = paginator({
   90:                           prev_text: '<i class="fa fa-angle-left"></i>',
   91:                           next_text: '<i class="fa fa-angle-right"></i>',
   92                            current: current,
   93:                           total: total,
   94:                           base: base,
   95:                           format: "{{ paginate_path | replace: ':num', '%d' }}",
   96                            mid_size: 1,
   97                            end_size: 1
   98                          });
   99:           $('nav.pagination').html(content);
  100        }
  101      })();

