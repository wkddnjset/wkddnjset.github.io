    5  
    6    // Write position in cookie
    7:   var timeout;
    8    $(window).on("scroll", function() {
    9:     clearTimeout(timeout);
   10      timeout = setTimeout(function () {
   11:       Cookies.set("scroll-cookie", ($(window).scrollTop() + "|" + rpath), { expires: 365, path: '' });
   12      }, 250);
   13    });
