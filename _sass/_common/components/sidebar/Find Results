 1755:   var b = control.context
 1756  
 1757    try {
 1758:     for ( var j = 1; j <= 20; j++ ) {
 1759:       for ( var i = 1; i <= 50; i++ ) {
 1760          if (
 1761            typeof ret === 'undefined' &&
 1762:           a.getImageData(i, j, 1, 1).data[3] !== b.getImageData(i, j, 1, 1).data[3]
 1763          ) {
 1764:           ret = false
 1765:           break
 1766:         } else if ( typeof ret === 'boolean' ) {
 1767:           break
 1768          }
 1769  
 ....
 1774      }
 1775  
 1776:     // Remove and clean from memory
 1777:     treat.remove()
 1778      control.remove()
 1779:     treat = null
 1780      control = null
 1781  
 1782      return ret
 1783:   } catch (e) {}
 1784:   return false
 1785  }
