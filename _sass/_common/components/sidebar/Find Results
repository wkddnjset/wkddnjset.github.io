  495: # Get more information on http://ibruce.info/2015/04/04/busuanzi/
  496: busuanzi_count:
  497:   # count values only if the other configs are false
  498:   enable: false
  499:   # custom uv span for the whole site
  500    site_uv: true
  501:   site_uv_header: <i class="fa fa-user"></i>
  502    site_uv_footer:
  503:   # custom pv span for the whole site
  504    site_pv: true
  505:   site_pv_header: <i class="fa fa-eye"></i>
  506    site_pv_footer:
  507:   # custom pv span for one page only
  508:   page_pv: true
  509:   page_pv_header: <i class="fa fa-file-o"></i>
  510:   page_pv_footer:
  511  
  512  
  513: # Tencent analytics ID
  514: # tencent_analytics:
  515  
  516: # Tencent MTA ID
  517: # tencent_mta:
  518  
  519  
  520: # Enable baidu push so that the blog will push the url to baidu automatically which is very helpful for SEO
  521: baidu_push: false
  522  
  523: # Google Calendar
  524: # Share your recent schedule to others via calendar page
  525  #
  526: # API Documentation:
  527: # https://developers.google.com/google-apps/calendar/v3/reference/events/list
  528: calendar:
  529:   enable: false
  530:   calendar_id: <required>
  531:   api_key: <required>
  532:   orderBy: startTime
  533:   offsetMax: 24
  534    offsetMin: 4
  535    timeZone:
  536:   showDeleted: false
  537    singleEvents: true
  538:   maxResults: 250
