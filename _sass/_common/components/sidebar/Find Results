 2140: // Create a new fake `<h-ruby>` element so the
 2141: // style sheets will render it as a polyfill,
 2142: // which also helps to avoid the UA style.
 2143: function createCustomRuby( frag ) {
 2144:   var $ruby = frag.firstChild
 2145:   var hruby = $.create( 'h-ruby' )
 2146  
 2147    hruby.innerHTML = $ruby.innerHTML
 2148:   $.setAttr( hruby, $ruby.attributes )
 2149:   hruby.normalize()
 2150    return hruby
 2151  }
 2152  
 2153: function simplifyRubyClass( elem ) {
 2154:   if ( !elem instanceof Element )  return elem
 2155:   var clazz = elem.classList
 2156  
 2157:   if      ( clazz.contains( 'pinyin' ))        clazz.add( 'romanization' )
 2158:   else if ( clazz.contains( 'romanization' ))  clazz.add( 'annotation' )
 2159:   else if ( clazz.contains( 'mps' ))           clazz.add( 'zhuyin' )
 2160:   else if ( clazz.contains( 'rightangle' ))    clazz.add( 'complex' )
 2161    return elem
 2162  }
 2163  
 2164  /**
 2165:  * Create and return a new `<h-ru>` element
 2166:  * according to the given contents
 2167   */
 2168: function createNormalRu( $rb, $rt, attr ) {
 2169:   var $ru = $.create( 'h-ru' )
 2170:   var $rt = $.clone( $rt )
 2171:   var attr = attr || {}
 2172:   attr.annotation = 'true'
 2173  
 2174:   if ( Array.isArray( $rb )) {
 2175:     $ru.innerHTML = $rb.map(function( rb ) {
 2176        if ( typeof rb === 'undefined' )  return ''
 2177        return rb.outerHTML 
 2178      }).join('') + $rt.outerHTML
 2179    } else {
 2180:     $ru.appendChild( $.clone( $rb ))
 2181:     $ru.appendChild( $rt )
 2182    }
 2183  
 2184:   $.setAttr( $ru, attr )
 2185    return $ru
 2186  }
 2187  
 2188  /**
 2189:  * Create and return a new `<h-ru>` element
 2190   * in Zhuyin form
 2191   */
 2192: function createZhuyinRu( $rb, $rt ) {
 2193:   var $rb = $.clone( $rb )
 2194  
 2195:   // Create an element to return
 2196:   var $ru = $.create( 'h-ru' )
 2197:   $ru.setAttribute( 'zhuyin', true )
 2198  
 2199    // - <h-ru zhuyin>
 ....
 2201    // -   <h-zhuyin>
 2202    // -     <h-yin></h-yin>
 2203:   // -     <h-diao></h-diao>
 2204    // -   </h-zhuyin>
 2205    // - </h-ru>
 2206:   $ru.appendChild( $rb )
 2207    $ru.innerHTML += getZhuyinHTML( $rt )
 2208    return $ru
 ....
 2210  
 2211  /**
 2212:  * Create a Zhuyin-form HTML string
 2213   */
 2214  function getZhuyinHTML( rt ) {
 2215:   // #### Explanation ####
 2216:   // * `zhuyin`: the entire phonetic annotation
 2217:   // * `yin`:    the plain pronunciation (w/out tone)
 2218:   // * `diao`:   the tone
 2219:   // * `len`:    the length of the plain pronunciation (`yin`)
 2220:   var zhuyin = typeof rt === 'string' ? rt : rt.textContent
 2221:   var yin, diao, len
 2222  
 2223:   yin  = zhuyin.replace( TYPESET.zhuyin.diao, '' )
 2224    len  = yin ? yin.length : 0
 2225:   diao = zhuyin
 2226:     .replace( yin, '' )
 2227:     .replace( /[\u02C5]/g, '\u02C7' )
 2228:     .replace( /[\u030D]/g, '\u0358' )
 2229:   return len === 0 ? '' : '<h-zhuyin length="' + len + '" diao="' + diao + '"><h-yin>' + yin + '</h-yin><h-diao>' + diao + '</h-diao></h-zhuyin>'
 2230  }
