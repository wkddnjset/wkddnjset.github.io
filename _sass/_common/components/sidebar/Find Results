 2569: function detectSpaceFont() {
 2570:   var div = $.create( 'div' )
 2571:   var ret
 2572  
 2573:   div.innerHTML = '<span>a b</span><span style="font-family: \'Han Space\'">a b</span>'
 2574:   body.appendChild( div )
 2575:   ret = div.firstChild.offsetWidth !== div.lastChild.offsetWidth
 2576    $.remove( div )
 2577    return ret
 2578  }
