   31:   // Sidebar float
   32:   function initAffix () {
   33:     var headerHeight = $('.header-inner').height();
   34:     var footerOffset = parseInt($('.main').css('padding-bottom'), 10);
   35  
   36:     /*jshint camelcase: false */
   37:     var sidebarTop = (CONFIG.sidebar.offset_float === 0) ?
   38:       headerHeight + CONFIG.sidebar.offset :
   39:       headerHeight;
   40:     /*jshint camelcase: true */
   41  
   42:     $('.sidebar-inner').affix({
   43        offset: {
