  897:   function escapeRegExp(s) {
  898:     return String(s).replace(/([.*+?^=!:${}()|[\]\/\\])/g, '\\$1')
  899    }
  900  
  901    function exposed() {
  902:     // Try deprecated arg signature first:
  903:     return deprecated.apply(null, arguments) || findAndReplaceDOMText.apply(null, arguments)
  904    }
  905  
  906:   function deprecated(regex, node, replacement, captureGroup, elFilter) {
  907:     if ((node && !node.nodeType) && arguments.length <= 2) {
  908:       return false
  909      }
  910:     var isReplacementFunction = typeof replacement == 'function'
  911:     if (isReplacementFunction) {
  912:       replacement = (function(original) {
  913:         return function(portion, match) {
  914:           return original(portion.text, match.startIndex)
  915          }
  916:       }(replacement))
  917      }
  918  
  919:     // Awkward support for deprecated argument signature (<0.4.0)
  920:     var instance = findAndReplaceDOMText(node, {
  921  
  922        find: regex,
  923  
  924:       wrap: isReplacementFunction ? null : replacement,
  925:       replace: isReplacementFunction ? replacement : '$' + (captureGroup || '&'),
  926  
  927:       prepMatch: function(m, mi) {
  928  
  929:         // Support captureGroup (a deprecated feature)
  930  
  931:         if (!m[0]) throw 'findAndReplaceDOMText cannot handle zero-length matches'
  932:         if (captureGroup > 0) {
  933:           var cg = m[captureGroup]
  934            m.index += m[0].indexOf(cg)
  935            m[0] = cg
  ...
  937       
  938          m.endIndex = m.index + m[0].length
  939:         m.startIndex = m.index
  940          m.index = mi
  941          return m
  ...
  944      })
  945      exposed.revert = function() {
  946:       return instance.revert()
  947      }
  948      return true
  ...
  950  
  951    /** 
  952:    * findAndReplaceDOMText
  953     * 
  954:    * Locates matches and replaces with replacementNode
  955     *
  956:    * @param {Node} node Element or Text node to search within
  957:    * @param {RegExp} options.find The regular expression to match
  958:    * @param {String|Element} [options.wrap] A NodeName, or a Node to clone
  959:    * @param {String|Function} [options.replace='$&'] What to replace each match with
  960:    * @param {Function} [options.filterElements] A Function to be called to check whether to
  961:    *	process an element. (returning true = process element,
  962:    *	returning false = avoid element)
  963     */
  964:   function findAndReplaceDOMText(node, options) {
  965      return new Finder(node, options)
  966    }
  ...
  968    exposed.NON_PROSE_ELEMENTS = {
  969      br:1, hr:1,
  970:     // Media / Source elements:
  971:     script:1, style:1, img:1, video:1, audio:1, canvas:1, svg:1, map:1, object:1,
  972      // Input elements
  973:     input:1, textarea:1, select:1, option:1, optgroup: 1, button:1
  974    }
  975    exposed.NON_CONTIGUOUS_PROSE_ELEMENTS = {
