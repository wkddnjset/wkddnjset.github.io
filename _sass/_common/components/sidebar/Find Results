 2479: var last$hwsIdx
 2480  
 2481: function apostrophe( portion ) {
 2482:   var $elmt = portion.node.parentNode
 2483  
 2484    if ( portion.index === 0 ) {
 2485:     last$hwsIdx = portion.endIndexInNode-2
 2486    }
 2487  
 2488    if (
 2489:     $elmt.nodeName.toLowerCase() === 'h-hws' && (
 2490:     portion.index === 1 || portion.indexInMatch === last$hwsIdx
 2491    )) {
 2492:     $elmt.classList.add( 'quote-inner' )
 2493    }
 2494    return portion.text
 ....
 2496  
 2497  function curveQuote( portion ) {
 2498:   var $elmt = portion.node.parentNode
 2499  
 2500:   if ( $elmt.nodeName.toLowerCase() === 'h-hws' ) {
 2501:     $elmt.classList.add( 'quote-outer' )
 2502    }
 2503    return portion.text
 2504  }
 2505  
 2506: $.extend( Han, {
 2507    renderHWS: function( context, strict ) {
 2508:     // Elements to be filtered according to the
 2509      // HWS rendering mode.
 2510:     var AVOID = strict
 2511:     ? 'textarea, code, kbd, samp, pre'
 2512:     : 'textarea'
 2513  
 2514:     var mode = strict ? 'strict' : 'base'
 2515:     var context = context || document
 2516:     var finder = Han.find( context )
 2517  
 2518      finder
 2519:     .avoid( AVOID )
 2520  
 2521:     // Basic situations:
 2522:     // - 字a => 字<hws/>a
 2523:     // - A字 => A<hws/>字
 2524:     .replace( Han.TYPESET.hws[ mode ][0], firstStepLabel )
 2525:     .replace( Han.TYPESET.hws[ mode ][1], firstStepLabel )
 2526  
 2527:     // Convert text nodes `<hws/>` into real element nodes:
 2528:     .replace( new RegExp( '(' + hws + ')+', 'g' ), real$hwsElmt )
 2529  
 2530:     // Deal with:
 2531      // - '<hws/>字<hws/>' => '字'
 2532      // - "<hws/>字<hws/>" => "字"
 2533:     .replace( /([\'"])\s(.+?)\s\1/g, apostrophe )
 2534  
 2535:     // Deal with:
 2536      // - <hws/>“字”<hws/>
 2537      // - <hws/>‘字’<hws/>
 2538:     .replace( /\s[‘“]/g, curveQuote )
 2539:     .replace( /[’”]\s/g, curveQuote )
 2540:     .normalize()
 2541  
 2542:     // Return the finder instance for future usage
 2543      return finder
 2544    }
 2545  })
