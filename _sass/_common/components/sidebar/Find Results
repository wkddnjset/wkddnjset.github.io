   76: 					if (!that.list || !width || !height) {
   77  						return;
   78  					}
   79  
   80: 					//Calculate thumbnail width/height and center it
   81: 					widthRatio  = width / thumbWidth;
   82: 					heightRatio = height / thumbHeight;
   83  
   84: 					parent = that.list.children().eq(i).find('a');
