 2232  /**
 2233:  * Normalize `ruby` elements
 2234   */
 2235: $.extend( Locale, {
 2236  
 2237:   // Address normalisation for both simple and complex
 2238:   // rubies (interlinear annotations)
 2239:   renderRuby: function( context, target ) {
 2240:     var target   = target || 'ruby'
 2241:     var $target  = $.qsa( target, context )
 2242  
 2243:     $.qsa( 'rtc', context )
 2244:     .concat( $target ).map( simplifyRubyClass )
 2245  
 2246:     $target
 2247:     .forEach(function( $ruby ) {
 2248:       var clazz = $ruby.classList
 2249:       var $new
 2250  
 2251:       if      ( clazz.contains( 'complex' ))  $new = renderComplexRuby( $ruby )
 2252:       else if ( clazz.contains( 'zhuyin' ))   $new = SUPPORT_IC ? renderInterCharRuby( $ruby ) : renderSimpleRuby( $ruby )
 2253  
 2254:       // Finally, replace it
 2255:       if ( $new )  $ruby.parentNode.replaceChild( $new, $ruby )
 2256      })
 2257    },
 2258  
 2259:   simplifyRubyClass:   simplifyRubyClass,
 2260    getZhuyinHTML:       getZhuyinHTML,
 2261    renderComplexRuby:   renderComplexRuby,
