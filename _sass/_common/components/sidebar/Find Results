 1008    /**
 1009:    * Finder -- encapsulates logic to find and replace.
 1010     */
 1011    function Finder(node, options) {
 1012  
 1013:     var preset = options.preset && exposed.PRESETS[options.preset]
 1014:     options.portionMode = options.portionMode || PORTION_MODE_RETAIN
 1015      if (preset) {
 1016:       for (var i in preset) {
 1017:         if (hasOwn.call(preset, i) && !hasOwn.call(options, i)) {
 1018            options[i] = preset[i]
 1019          }
 ....
 1023      this.node = node
 1024      this.options = options
 1025:     // ENable match-preparation method to be passed as option:
 1026:     this.prepMatch = options.prepMatch || this.prepMatch
 1027      this.reverts = []
 1028:     this.matches = this.search()
 1029:     if (this.matches.length) {
 1030:       this.processMatches()
 1031      }
 1032  
 ....
 1036  
 1037      /**
 1038:      * Searches for all matches that comply with the instance's 'match' option
 1039       */
 1040:     search: function() {
 1041  
 1042:       var match
 1043:       var matchIndex = 0
 1044:       var offset = 0
 1045:       var regex = this.options.find
 1046:       var textAggregation = this.getAggregateText()
 1047:       var matches = []
 1048:       var self = this
 1049:       regex = typeof regex === 'string' ? RegExp(escapeRegExp(regex), 'g') : regex
 1050:       matchAggregation(textAggregation)
 1051:       function matchAggregation(textAggregation) {
 1052:         for (var i = 0, l = textAggregation.length; i < l; ++i) {
 1053  
 1054:           var text = textAggregation[i]
 1055            if (typeof text !== 'string') {
 1056:             // Deal with nested contexts: (recursive)
 1057:             matchAggregation(text)
 1058              continue
 1059            }
 1060  
 1061:           if (regex.global) {
 1062:             while (match = regex.exec(text)) {
 1063:               matches.push(self.prepMatch(match, matchIndex++, offset))
 1064              }
 1065            } else {
 1066:             if (match = text.match(regex)) {
 1067:               matches.push(self.prepMatch(match, 0, offset))
 1068              }
 1069            }
 ....
 1073        }
 1074  
 1075:       return matches
 1076      },
 1077  
 1078      /**
 1079:      * Prepares a single match with useful meta info:
 1080       */
 1081:     prepMatch: function(match, matchIndex, characterOffset) {
 1082  
 1083:       if (!match[0]) {
 1084:         throw new Error('findAndReplaceDOMText cannot handle zero-length matches')
 1085        }
 1086     
 1087:       match.endIndex = characterOffset + match.index + match[0].length
 1088:       match.startIndex = characterOffset + match.index
 1089:       match.index = matchIndex
 1090:       return match
 1091      },
 1092  
 1093      /**
 1094:      * Gets aggregate text within subject node
 1095       */
