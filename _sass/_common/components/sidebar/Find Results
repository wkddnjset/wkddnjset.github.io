 1961: Locale.initCond = function( target ) {
 1962:   var target = target || root
 1963:   var ret = ''
 1964:   var clazz
 1965  
 1966:   for ( var feature in Locale.support ) {
 1967:     clazz = ( Locale.support[ feature ] ? '' : 'no-' ) + feature
 1968  
 1969:     target.classList.add( clazz )
 1970:     ret += clazz + ' '
 1971    }
 1972    return ret
 1973  }
