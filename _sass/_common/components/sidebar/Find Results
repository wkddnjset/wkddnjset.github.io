 1731:   canvas.height = '20'
 1732:   canvas.style.display = 'none'
 1733  
 1734:   body.appendChild( canvas )
 1735  
 1736:   context = canvas.getContext( '2d' )
 1737:   context.textBaseline = 'top'
 1738:   context.font = '15px ' + font + ', sans-serif'
 1739:   context.fillStyle = 'black'
 1740:   context.strokeStyle = 'black'
 1741    context.fillText( text, 0, 0 )
 1742  
 1743    return {
 1744:     node: canvas,
 1745      context: context,
 1746      remove: function() {
 1747:       $.remove( canvas, body )
 1748      }
 1749    }
 1750  }
