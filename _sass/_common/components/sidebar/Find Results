    2: {% if site.paginate > 0 %}{% assign posts = paginator.posts %}{% endif %}
    3  
    4: <section id="posts" class="posts-expand">
    5    {% for post in posts %}
    6:     {% assign is_index = true %}
    7:     {% include _macro/post.html %}
    8    {% endfor %}
    9  </section>
