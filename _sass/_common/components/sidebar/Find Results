/Users/deep-core-01/Desktop/blog/wkddnjset.github.io/_includes/_helper/paginator.html:
    1: {% comment %} paginator(paginator, prev_text, next_text) {% endcomment %}
    2: {% capture paginator %}
    3:   {% assign end_size = 1 %}
    4:   {% assign mid_size = 1 %}
    5:   {% assign current =  paginator.page %}
    6:   {% capture current_page %}
    7:     <span class="page-number current">{{ current }}</span>
    8:   {% endcapture %}
    9:   {% if paginator.previous_page %}
   10:     <a class="extend prev" rel="prev" href="{{ paginator.previous_page_path | relative_url }}">{{ prev_text }}</a>
   11    {% endif %}
   12  
   13:   {% assign total = paginator.total_pages %}
   14  
   15:   {% assign left_end = end_size %}
   16:   {% assign right_end = total | minus: end_size | plus: 1 %}
   17:   {% assign left_mid = current | minus: mid_size %}
   18:   {% assign right_mid = current | plus: mid_size %}
   19  
   20:   {% if left_end >= current %}{% assign left_end = current | minus: 1 %}{% endif %}
   21  
   22:   {% assign current_plus_one = current | plus: 1 %}
   23:   {% if right_end <= current_plus_one %}{% assign right_end = current_plus_one %}{% endif %}
   24  
   25:   {% if left_mid <= end_size %}{% assign left_mid = left_mid | plus: end_size %}{% endif %}
   26  
   27:   {% assign total_minus_end_size = total | minus: end_size %}
   28:   {% if right_mid > total_minus_end_size %}{% assign right_mid = right_mid | minus: end_size %}{% endif %}
   29  
   30:   {% assign space_html = '<span class="space">&hellip;</span>' %}
   31  
   32    {% for i in (1..left_end) %}
   33:     {% assign href = site.paginate_path | relative_url | replace: '//', '/' | replace: ':num', i %}
   34      {% if i == 1 %}
   35:       {% assign href = "/" | relative_url %}
   36      {% endif %}
   37:     <a class="page-number" href="{{ href }}">{{ i }}</a>
   38    {% endfor %}
   39  
   40:   {% assign current_minus_end_size_minus_mid_size = current | minus: end_size | minus: mid_size %}
   41    {% if current_minus_end_size_minus_mid_size > 1 %}
   42:     {{ space_html }}
   43    {% endif %}
   44  
   45:   {% assign current_minus_one = current | minus: 1 %}
   46    {% if left_mid > left_end %}
   47      {% for i in (left_mid..current_minus_one) %}
   48:       <a class="page-number" href="{{ site.paginate_path | relative_url | replace: '//', '/' | replace: ':num', i }}">{{ i }}</a>
   49      {% endfor %}
   50    {% endif %}
   51:   {{ current_page }}
   52  
   53    {% if right_mid < right_end %}
   54      {% for i in (current_plus_one..right_mid) %}
   55:       <a class="page-number" href="{{ site.paginate_path | relative_url | replace: '//', '/' | replace: ':num', i }}">{{ i }}</a>
   56      {% endfor %}
   57    {% endif %}
   58  
   59:   {% assign total_minus_end_size_minus_mid_size = total_minus_end_size | minus: mid_size %}
   60:   {% if total_minus_end_size_minus_mid_size > current %}
   61:     {{ space_html }}
   62    {% endif %}
   63  
   64:   {% for i in (right_end..total) %}
   65:     <a class="page-number" href="{{ site.paginate_path | relative_url | replace: '//', '/' | replace: ':num', i }}">{{ i }}</a>
   66    {% endfor %}
   67  
   68:   {% if current < total %}
   69:     <a class="extend prev" rel="next" href="{{ paginator.next_page_path | relative_url }}">{{ next_text }}</a>
   70    {% endif %}
   71: {% endcapture %}
   72: {% assign paginator = paginator | normalize_whitespace %}
   73  

/Users/deep-core-01/Desktop/blog/wkddnjset.github.io/_includes/_helper/tagcloud.html:
    1: {% comment %} tagcloud(min_font, max_font, amount, color, start_color, end_color) {% endcomment %}
    2: {% capture tagcloud %}
    3:   {% assign tags = site.tags %}
    4:   {% assign max = max_font %}
    5:   {% assign min = min_font %}
    6:   {% assign sizes = '' | split: '' %}
    7:   {% assign unit = 'px' %}
    8:   {% assign start_color = start_color | split: ',' %}
    9:   {% assign end_color = end_color | split: ',' %}
   10  
   11:   {% for tag in tags %}
   12:     {% if forloop.index > amount %}
   13:       {% break %}
   14      {% endif %}
   15:     {% assign sizes = sizes | push: tag[1].size %}
   16    {% endfor %}
   17  
   18:   {% assign sizes = sizes | uniq | sort %}
   19:   {% assign length = sizes.size | minus: 1 %}
   20  
   21:   {% for tag in tags %}
   22:     {% if forloop.index > amount %}
   23:       {% break %}
   24      {% endif %}
   25  
   26:     {% assign ratio = 0 %}
   27  
   28      {% if length > 0 %}
   29        {% for size in sizes %}
   30:         {% if size == tag[1].size %}
   31:           {% assign ratio = forloop.index0 | times: 1.0 | divided_by: length %}
   32:           {% break %}
   33          {% endif %}
   34        {% endfor %}
   35      {% endif %}
   36  
   37:     {% assign tmp = max | minus: min | times: ratio %}
   38:     {% assign size = tmp | plus: min %}
   39:     {% assign tmp = size | round: 2 %}
   40  
   41:     {% assign style = 'font-size: ' | append: tmp | append: unit | append: ';' %}
   42  
   43      {% if color %}
   44:       {% assign mid_color = '' | split: '' %}
   45        {% for i in (0..2) %}
   46:         {% assign tmp = end_color[i] | minus: start_color[i] | times: ratio %}
   47:         {% assign tmp = start_color[i] | plus: tmp | round %}
   48:         {% assign mid_color = mid_color | push: tmp %}
   49        {% endfor %}
   50:       {% assign mid_color = 'rgb(' | append: mid_color[0] | append: ',' | append: mid_color[1] | append: ',' | append: mid_color[2] | append: ')' %}
   51:       {% assign style = style | append: 'color: ' | append: mid_color %}
   52      {% endif %}
   53:     {% assign tag_url_encode = tag[0] | url_encode | replace: '+', '%20' %}
   54:     <a href="{{ '/tag/#/' | relative_url | append: tag_url_encode }}" style="{{ style }}">{{ tag[0] }}</a>
   55    {% endfor %}
   56  
   57: {% endcapture %}
   58: {% assign tagcloud = tagcloud | normalize_whitespace %}
   59  

/Users/deep-core-01/Desktop/blog/wkddnjset.github.io/_includes/_helper/toc.html:
    1: {% comment %} toc(str, class, list_number) {% endcomment %}
    2: {% capture toc %}
    3:   {% assign first_level = nil %}
    4:   {% assign str_stack = str | split: '<h0>' %}
    5:   {% assign level_stack = '0' | split: '' %}
    6:   {% assign title_stack = '' | split: '' | push: nil %}
    7:   {% assign id_stack = '' | split: '' | push: nil %}
    8:   {% assign num_stack = '0' | split: '' %}
    9  
   10:   {% assign num_array = '0' | split: '' %}
   11  
   12:   {% assign first = true %}
   13    {% for i in (1..100) %}
   14:     {% if str_stack.size == 0 %}
   15:       {% break %}
   16      {% endif %}
   17:     {% assign str = str_stack | last %}
   18:     {% assign level = level_stack | last %}
   19:     {% assign toc_title = title_stack | last %}
   20:     {% assign id = id_stack | last %}
   21:     {% assign num = num_stack | last %}
   22  
   23:     {% assign str_stack = str_stack | pop %}
   24:     {% assign level_stack = level_stack | pop %}
   25:     {% assign title_stack = title_stack | pop %}
   26:     {% assign id_stack = id_stack | pop %}
   27:     {% assign num_stack = num_stack | pop %}
   28  
   29      {% if first_level %}
   30:       {% assign deep = num_array.size | minus: 1 | minus: level %}
   31  
   32        {% for i in (1..deep) %}
   ..
   37          {% if deep < 0 %}
   38            {% for i in (deep..-1) %}
   39:             <ol class="{{ class }}-child">
   40            {% endfor %}
   41          {% else %}
   ..
   44      {% endunless %}
   45  
   46:       {% assign first = false %}
   47      {% endif %}
   48  
   49:     {% assign num_array = num_array | slice: 0, level %}
   50:     {% assign num_array = num_array | push: num %}
   51  
   52:     {% if toc_title and num_array.size != 0 %}
   53:       <li class="{{ class }}-item {{ class }}-level-{{ level }}">
   54:         <a class="{{ class }}-link" href="#{{ id }}">
   55            {% if list_number %}
   56:             {% assign tmp = num_array | slice: first_level, 7 %}
   57:             {% capture number %}{% for i in tmp %}.{{i}}{% endfor %}{% endcapture %}
   58:             {% assign number = number | strip | remove_first: '.' %}
   59:             <span class="{{ class }}-number">{{ number }}</span>
   60            {% endif %}
   61:           <span class="{{ class }}-text">{{ toc_title }}</span>
   62:         </a>
   63      {% endif %}
   64  
   65:     {% assign level = level | plus: 1 %}
   66  
   67      {% if level > 6 %}
   ..
   69      {% endif %}
   70  
   71:     {% assign tag_begin = '<h' | append: level %}
   72:     {% assign tag_end = '</h' | append: level | append: '>'%}
   73  
   74:     {% unless str contains tag_end %}
   75:       {% assign str_stack = str_stack | push: str %}
   76:       {% assign level_stack = level_stack | push: level %}
   77:       {% assign title_stack = title_stack | push: nil %}
   78:       {% assign id_stack = id_stack | push: nil %}
   79:       {% assign num_stack = num_stack | push: 0 %}
   80        {% continue %}
   81      {% endunless %}
   82  
   83      {% unless first_level %}
   84:       {% assign first_level = level %}
   85      {% endunless %}
   86  
   87:     {% assign tags = str | split: tag_begin | shift | reverse %}
   88  
   89:     {% for tag in tags %}
   90:       {% assign tmp = tag | split: tag_end %}
   91:       {% assign str = tmp[1] | strip %}
   92  
   93:       {% assign tmp = tmp[0] | strip | split: '>' %}
   94:       {% assign toc_title = tmp | shift | join: '>' | strip_html %}
   95  
   96:       {% assign tmp = tmp[0] | strip | split: 'id=' %}
   97:       {% assign id = tmp[1] | strip | replace: '"', '' %}
   98  
   99:       {% assign str_stack = str_stack | push: str %}
  100:       {% assign level_stack = level_stack | push: level %}
  101:       {% assign title_stack = title_stack | push: toc_title %}
  102:       {% assign id_stack = id_stack | push: id %}
  103:       {% assign num_stack = num_stack | push: forloop.rindex %}
  104      {% endfor %}
  105    {% endfor %}
  106: {% endcapture %}
  107: {% assign toc = toc | normalize_whitespace %}
  108  {% if toc.size != 0 %}
  109:   {% capture toc %}
  110:     <ol class={{ class }}>
  111        {{ toc }}
  112      </ol>
  113:   {% endcapture %}
  114  {% endif %}
  115  

