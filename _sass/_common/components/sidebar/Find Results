   47  
   48: 			if (arguments.length > 1) {
   49: 				attributes = extend({
   50: 					path: '/'
   51: 				}, api.defaults, attributes);
   52  
   53: 				if (typeof attributes.expires === 'number') {
   54: 					var expires = new Date();
