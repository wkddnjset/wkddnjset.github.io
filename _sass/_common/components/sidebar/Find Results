 1460: function createBDGroup( portion ) {
 1461:   var clazz = portion.index === 0 && portion.isEnd
 1462:     ? 'biaodian cjk'
 1463:     : 'biaodian cjk portion ' + (
 1464        portion.index === 0
 1465        ? 'is-first'
 ....
