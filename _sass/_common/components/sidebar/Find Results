 1476: function createBDChar( char ) {
 1477:   var div     = $.create( 'div' )
 1478:   var unicode = char.charCodeAt( 0 ).toString( 16 )
 1479  
 1480    div.innerHTML = (
 1481:     '<h-char unicode="' + unicode +
 1482:     '" class="biaodian cjk ' + getBDType( char ) +
 1483:     '">' + char + '</h-char>'
 1484    )
 1485    return div.firstChild
 1486  }
